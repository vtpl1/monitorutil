{
	"name": "monitorutil",
	"version": "0.0.1",
	"port-version": 0,
	"description": "A simple wrapper around grpc",
	"homepage": "https://github.com/vtpl1/monitorutil",
	"builtin-baseline": "9ba75e6e174b30b97afc5c8c3170c5d25c548b32",
	"license": null,
	"dependencies": [
		{
			"name": "vcpkg-cmake-get-vars",
			"host": true
		},
		{
			"name": "vcpkg-cmake",
			"host": true
		},
		{
			"name": "vcpkg-cmake-config",
			"host": true
		}
	],
	"default-features": [
		"all"
	],
	"features": {
		"basic": {
			"description": "Basic functions",
			"dependencies": [
				{
					"name": "logutil"
				}
			]
		},
		"grpc": {
			"description": "Basic functions",
			"dependencies": [
				{
					"name": "monitorutil",
					"features": [
						"basic"
					]
				},
				"protobuf",
				"grpc"
			]
		},
		"client_exe": {
			"description": "Client executable for test",
			"dependencies": [
				{
					"name": "monitorutil",
					"features": [
						"grpc"
					]
				},
				"poco"
			]
		},
		"server_exe": {
			"description": "Server executable for test",
			"dependencies": [
				{
					"name": "monitorutil",
					"features": [
						"grpc"
					]
				},
				"poco"
			]
		},
		"all": {
			"description": "Build with all",
			"dependencies": [
				{
					"name": "monitorutil",
					"features": [
						"basic",
						"grpc",
						"client_exe",
						"server_exe"
					]
				},
				"catch2",
				"abseil"
			]
		}
	}
}